{
  "compilerOptions": {
    "sourceMap": true,
    "ignoreDeprecations": "5.0",
    "target": "ES5",
    "module": "ES6",
    "moduleResolution": "node",
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "strictPropertyInitialization": false,
    "noImplicitThis": false
  },
  "exclude": [
    "test/uniapp2",
    "test/uniapp3",
    "test/alipay",
    "test/miniprogram",
    "test/script"
  ]
}
